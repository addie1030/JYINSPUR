                </form>
            </field>
        </record>

        <record id="action_window_stock_cost_maintenance_setting" model="ir.actions.act_window">
            <field name="name">Storage Cost Maintenance</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.maintenance.cost</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_stock_cost_maintenance_form"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="binding_model_id" ref="stock.model_stock_move"/>
        </record>
    </data>
