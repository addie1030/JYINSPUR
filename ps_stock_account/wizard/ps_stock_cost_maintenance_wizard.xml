        <record id="view_stock_cost_maintenance_form" model="ir.ui.view">
            <field name="name">Storage Cost Maintenance</field>
            <field name="model">ps.maintenance.cost</field>
            <field name="arch" type="xml">
                <form string="Storage Cost Maintenance">
                    <p>Are you sure to import from the price maintenance table?</p>
                    <footer>
                        <button string="Confirm" name="confirm_set_button" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
