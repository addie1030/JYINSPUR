<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ps_mrp_complete_warehouse_qty_wizard" model="ir.ui.view">
            <field name="name">ps mrp complete warehouse quantity wizard</field>
            <field name="model">ps.mrp.complete.warehouse.qty.wizard</field>
            <field name="arch" type="xml">
                <form string="Complete Warehouse Quantity">
                    <sheet>
                        <group>
                            <group>
                                <field name="period_id" required="1"/>
                            </group>
                        </group>
                        <footer>
                            <button name="complete" type="object"
                                    string="Complete Warehouse Quantity" class="oe_highlight"/>
                            <button class="btn btn-secondary oe_button oe_form_button" special="cancel"
                                    string="Cancel"/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>