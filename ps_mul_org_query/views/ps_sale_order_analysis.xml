<odoo>
    <data>
        <record id="ps_ps_sale_order_analysis_tree" model="ir.ui.view">
        <field name="name">ps.sale.order.analysis.view</field>
        <field name="model">ps.sale.order.analysis</field>
        <field name="arch" type="xml">
                <tree>
                    <field name="sale_partner_id"/>
                    <field name="purchase_partner_id"/>
                    <field name="sale_name"/>
                    <field name="product_id"/>
                    <field name="product_uom"/>
                    <field name="product_uom_qty"/>
                    <field name="state" />
                    <field name="price_reduce_taxexcl" />
                    <field name="price_tax"/>
                    <field name="price_reduce_taxinc"/>
                    <field name="amt_invoiced"/>
                    <!--<field name="out_name"/>-->
                    <!--<field name="in_name"/>-->
                </tree>
        </field>
        </record>

        <record id='action_ps_sale_order_analysis' model='ir.actions.act_window' >
            <field name='name'>Ps Sale Order Analysis</field>
            <field name='res_model'>ps.sale.order.analysis</field>
            <field name='view_mode'>tree</field>
            <field name="domain">[]</field>
        </record>
        <menuitem id="menu_ps_sale_order_analysis"
              name="Ps Sale Order Analysis"
              parent="sale.menu_sale_report"
              action="action_ps_sale_order_analysis"
              sequence="7"
              groups="base.group_user"/>
        <menuitem id="menu_ps_purchase_order_analysis"
              name="Ps Purchase Order Analysis"
              parent="purchase_analysis_report"
              action="action_ps_sale_order_analysis"
              sequence="7"
              groups="base.group_user"/>
    </data>
</odoo>