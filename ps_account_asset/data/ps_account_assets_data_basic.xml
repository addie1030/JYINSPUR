<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <!--预置资产用途字典-->
    <data noupdate="1">
        <record id="ps_account_attribute_assets_use1" model="account.assets.use">
            <field name="code">1</field>
            <field name="name">办公用</field>
        </record>
        <record id="ps_account_attribute_assets_use2" model="account.assets.use">
            <field name="code">2</field>
            <field name="name">生产用</field>
        </record>
        <record id="ps_account_attribute_assets_use3" model="account.assets.use">
            <field name="code">3</field>
            <field name="name">交通用</field>
        </record>
        <record id="ps_account_attribute_assets_use4" model="account.assets.use">
            <field name="code">4</field>
            <field name="name">其他用途</field>
        </record>
    </data>
    <!--预置资产状态字典-->
    <data noupdate="0">
        <record id="ps_account_attribute_assets_status1" model="account.assets.status">
            <field name="code">1</field>
            <field name="name">在用计提</field>
            <field name="ps_assets_depreciate">1</field>
        </record>
        <record id="ps_account_attribute_assets_status2" model="account.assets.status">
            <field name="code">2</field>
            <field name="name">在用不提</field>
        </record>
        <record id="ps_account_attribute_assets_status3" model="account.assets.status">
            <field name="code">3</field>
            <field name="name">闲置不提</field>
        </record>
    </data>
    <!--预置资产减少原因-->
    <data noupdate="0">
        <record id="ps_account_attribute_assets_reducefor1" model="account.assets.reducefor">
            <field name="code">01</field>
            <field name="name">投资转出</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor2" model="account.assets.reducefor">
            <field name="code">02</field>
            <field name="name">捐赠转出</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor3" model="account.assets.reducefor">
            <field name="code">03</field>
            <field name="name">债务重组</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor4" model="account.assets.reducefor">
            <field name="code">04</field>
            <field name="name">非货币性交易</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor5" model="account.assets.reducefor">
            <field name="code">05</field>
            <field name="name">调出</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor6" model="account.assets.reducefor">
            <field name="code">06</field>
            <field name="name">盘亏</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor7" model="account.assets.reducefor">
            <field name="code">07</field>
            <field name="name">出售</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor8" model="account.assets.reducefor">
            <field name="code">08</field>
            <field name="name">报废</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor9" model="account.assets.reducefor">
            <field name="code">09</field>
            <field name="name">毁损</field>
        </record>
        <record id="ps_account_attribute_assets_reducefor0" model="account.assets.reducefor">
            <field name="code">99</field>
            <field name="name">其他</field>
        </record>

    </data>

</odoo>
