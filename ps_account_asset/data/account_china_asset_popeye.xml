<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data noupdate="1">
        <record forcecreate="True" id="asset_gdzjff_001" model="ps.asset.gdzjff">
            <field name="name">工作量法</field>
            <field name="zjl">False</field>
            <field name="zje">False</field>
            <field name="yzje">{@GDZCMX_BYGL}*!({@GDZCMX_JTYZ}-!{@GDZCMX_JTJZZB}-!{@GDZCMX_JTJCZ})/!{@GDZCMX_JTGZL}</field>
            <field name="jesm">本月工作量*(资产原值-减值准备-净残值)/总工作量</field>
            <field name="yzjl"></field>
            <field name="jlsm"></field>
            <field name="jszq">M</field>
            <field name="jsgs"></field>
            <field name="sffc">False</field>
        </record>
        <record forcecreate="True" id="asset_gdzjff_002" model="ps.asset.gdzjff">
            <field name="name">平均年限法(一)</field>
            <field name="zjl">False</field>
            <field name="zje">False</field>
            <field name="yzje">({@GDZCMX_JTYZ}-!{@GDZCMX_JTJZZB})*!{@GDZCMX_YZJL}</field>
            <field name="jesm">(资产原值-减值准备)*月折旧率</field>
            <field name="yzjl">({1}-!{@GDZCMX_JTJCZL})/!{@GDZCMX_JTSYYF}</field>
            <field name="jlsm">(1-净残值率)/使用期限(月)</field>
            <field name="jszq">M</field>
            <field name="jsgs"></field>
            <field name="sffc">False</field>
        </record>
        <record forcecreate="True" id="asset_gdzjff_003" model="ps.asset.gdzjff">
            <field name="name">平均年限法(二)</field>
            <field name="zjl">False</field>
            <field name="zje">False</field>
            <field name="yzje">({@GDZCMX_JTYZ}-!{@GDZCMX_JTJZZB}-!{@GDZCMX_JTLJZJ}-!{@GDZCMX_JTJCZ})/!({@GDZCMX_JTSYYF}-!{@GDZCMX_YTYF})</field>
            <field name="jesm">(资产原值-减值准备-累计折旧-净残值)/(使用期限(月)-已提月份)</field>
            <field name="yzjl">({1}-!{@GDZCMX_JTJCZL})/!{@GDZCMX_JTSYYF}</field>
            <field name="jlsm">(1-净残值率)/使用期限(月)</field>
            <field name="jszq">M</field>
            <field name="jsgs"></field>
            <field name="sffc">False</field>
        </record>
        <record forcecreate="True" id="asset_gdzjff_004" model="ps.asset.gdzjff">
            <field name="name">双倍余额递减法</field>
            <field name="zjl">False</field>
            <field name="zje">False</field>
            <field name="yzje">({@GDZCMX_JTYZ}-!{@GDZCMX_JTLJZJ}-!{@GDZCMX_JTJZZB})*!{@GDZCMX_YZJL}</field>
            <field name="jesm">(资产原值 - 累计折旧-减值准备)*月折旧率</field>
            <field name="yzjl">{2}/!{@GDZCMX_JTSYYF}</field>
            <field name="jlsm">2/使用期限(月)</field>
            <field name="jszq">M</field>
            <field name="jsgs"></field>
            <field name="sffc">False</field>
        </record>
        <record forcecreate="True" id="asset_gdzjff_005" model="ps.asset.gdzjff">
            <field name="name">年数总和法</field>
            <field name="zjl">False</field>
            <field name="zje">False</field>
            <field name="yzje">({@GDZCMX_JTYZ}-!{@GDZCMX_JTJZZB}-!{@GDZCMX_JTJCZ})*!{@GDZCMX_YZJL}</field>
            <field name="jesm">(资产原值-减值准备-净残值)*月折旧率</field>
            <field name="yzjl">(ceiling(({@GDZCMX_JTSYYF}-!{@GDZCMX_YTYF})/!{12}))/!(ceiling({@GDZCMX_JTSYYF}/!{12})*!(ceiling({@GDZCMX_JTSYYF}/!{12})+!{1})/!{2})/!{12}</field>
            <field name="jlsm">进位取整((使用期限(月) - 已提月份)/12)/(年数总和*12)</field>
            <field name="jszq">M</field>
            <field name="jsgs"></field>
            <field name="sffc">False</field>
        </record>
    </data>

