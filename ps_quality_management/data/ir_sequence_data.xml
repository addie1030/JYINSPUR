<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_quality_check_order" model="ir.sequence">
            <field name="name">Quality Check Order</field>
            <field name="code">ps.quality.check.order</field>
            <field name="prefix">QCO#</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

         <record id="seq_ps_quality_testing_item" model="ir.sequence">
            <field name="name">Quality Testing Item</field>
            <field name="code">ps.quality.testing.item</field>
            <field name="prefix">QTI#</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_ps_quality_inspection_plan" model="ir.sequence">
            <field name="name">Quality Testing Inspection Plan</field>
            <field name="code">ps.quality.inspection.plan</field>
            <field name="prefix">QIP#</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_ps_quality_defect_disposal" model="ir.sequence">
            <field name="name">Quality Defect Disposal</field>
            <field name="code">ps.quality.defect.disposal</field>
            <field name="prefix">QDD#</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_ps_quality_inventory_check_request" model="ir.sequence">
            <field name="name">Quality Inventory Check Request</field>
            <field name="code">ps.quality.inventory.check.request</field>
            <field name="prefix">QICR#</field>
            <field name="padding">6</field>
        </record>

        <record id="seq_ps_quality_sampling_plan" model="ir.sequence">
            <field name="name">Quality Testing Sampling Plan</field>
            <field name="code">ps.quality.sampling.plan</field>
            <field name="prefix">QSS#</field>
            <field name="padding">6</field>
        </record>
    </data>
</odoo>