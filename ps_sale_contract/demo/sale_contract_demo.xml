<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="partner_demo_1" model="res.partner">
            <field name="name">bing</field>
            <field name="email">bing@example.com</field>
            <field name="supplier" eval="False"/>
            <field name="customer" eval="True"/>
            <field name="street">858 Lynn Street</field>
            <field name="city">Bayonne</field>
            <field name="zip">07002</field>
            <field name="country_id" ref="base.us"/>
            <field name="phone">(683)-556-5104</field>
        </record>

        <record id="res_partner_address_demo_1" model="res.partner">
            <field name="name">bing liu</field>
            <field name="street">langchao road</field>
            <field name="parent_id" eval="ref('ps_sale_contract.partner_demo_1')"/>
            <field name="function">Service Manager</field>
            <field name="email">willie.burke80@example.com</field>
            <field name="phone">(846)-523-2111</field>
            <field name="supplier">1</field>
            <field eval="0" name="customer"/>
        </record>

        <record id="sale_contract_1" model="ps.sale.contract">
            <field name="customer_id" ref="ps_sale_contract.partner_demo_1"/>
            <field name="company_id" ref="base.main_company"/>
            <field name="pricelist_id" ref="product.list0"/>
            <field name="currency_id" ref="base.CNY"/>
            <field name="valid_from" eval="DateTime.today().strftime('%Y-%m-%d')"/>
            <field name="valid_days">30</field>
            <field name="line_ids" model="ps.sale.contract.line" eval="[(0, 0, {
                'product_id': ref('product.product_product_1'),
                'quantity': 100,
            })]"/>
        </record>
    </data>
</odoo>