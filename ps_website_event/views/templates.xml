<odoo>
   <template id="ps_registration_attendee_details" inherit_id="website_event.registration_attendee_details">
       <xpath expr="//main[hasclass('modal-body')]"  position="replace">
           <main class="modal-body">
                <div class="container">
                    <t t-set="counter_type" t-value="1"/>
                    <t t-set="counter" t-value="0"/>
                    <t t-foreach="tickets" t-as="ticket">
                        <h4 class="page-header mt16">
                            <strong>
                                Ticket Type #<t t-raw="counter_type"/>: <t t-esc="ticket['name']"/>
                                <t t-if="ticket['price'] == 0">(Free)</t>
                            </strong>
                        </h4>
                        <t t-foreach="range(1, ticket['quantity'] + 1)" t-as="att_counter" name="attendee_loop1">
                            <t t-set="counter" t-value="counter + 1"/>
                            <div class="row mb4">
                                <t t-set="attendee_placeholder">Attendee #%s</t>
                                <t t-set="email_placeholder">Email #%s</t>
                                <t t-set="tel_placeholder">Phone #%s</t>
                                <t t-set="gender_placeholder">Gender #%s</t>
                                <div class="col-md-3"><input class='form-control' type='text' t-attf-name="#{counter}-name" required="This field is required" t-att-placeholder="attendee_placeholder %counter"/></div>
                                <div class="col-md-4"><input class='form-control' type='email' t-attf-name="#{counter}-email" required="This field is required" t-att-placeholder="email_placeholder %counter"/></div>
                                <div class="col-md-3"><input class='form-control' type='tel' t-attf-name="#{counter}-phone" t-att-placeholder="tel_placeholder %counter"/></div>
                                <!--<div class="col-md-3"><input class='form-control' t-attf-name="#{counter}-gender" t-att-placeholder="gender_placeholder %counter"/></div>-->
                                <div class="col-md-2"><select class='form-control' t-attf-name="#{counter}-gender">
                                            <option value="male">Male</option>
                                            <option value="female">Female</option>
                                        </select></div>
                                <input class='hidden' type='text' t-attf-name="#{counter}-ticket_id" t-attf-value="#{ticket['id']}"/>
                            </div>
                            <div class="row mb4">
                                <t t-set="company_placeholder">Company #%s</t>
                                <t t-set="position_placeholder">Position #%s</t>
                                <t t-set="state_placeholder">State #%s</t>
                                <t t-set="relationship_placeholder">Relationship #%s</t>
                                <div class="col-md-3"><input class='form-control' t-attf-name="#{counter}-company_name" t-att-placeholder="company_placeholder %counter"/></div>
                                <div class="col-md-4"><input class='form-control' t-attf-name="#{counter}-position" t-att-placeholder="position_placeholder %counter"/></div>
                                <div class="col-md-3"><input class='form-control' t-attf-name="#{counter}-state_id" t-att-placeholder="state_placeholder %counter"/></div>
                                <!--<div class="col-md-3">-->
                                    <!--&lt;!&ndash;<label class="control-label label-optional" for="state_id">State / Province</label>&ndash;&gt;-->
                                    <!--<select class="form-control" t-attf-name="#{counter}-state_id" t-att-placeholder="state_placeholder %counter">-->
                                        <!--<t t-foreach="states or []" t-as="state">-->
                                            <!--<option t-att-value="state.id" style="display:none;">-->
                                                <!--<t t-esc="state.name" />-->
                                            <!--</option>-->
                                        <!--</t>-->
                                    <!--</select>-->
                                <!--</div>-->
                                <div class="col-md-2"><select class='form-control' t-attf-name="#{counter}-relationship">
                                            <option value="customer">Customer</option>
                                            <option value="cloud">Cloud Partner</option>
                                            <option value="employee">Employee</option>
                                            <option value="other">Other</option>
                                    </select>
                                </div>
                                <input class='hidden' type='text' t-attf-name="#{counter}-ticket_id" t-attf-value="#{ticket['id']}"/>
                            </div>
                            <hr style="height:3px;border:none;border-top:3px solid #EFEFEF;" />
                        </t>
                        <t t-set="counter_type" t-value="counter_type + 1"/>
                    </t>
                </div>
            </main>
        </xpath>
    </template>
    <template id="assets_frontend" inherit_id="web.assets_frontend" name="Portal Assets">
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/ps_website_event/static/src/js/ps_website_event_tour.js"></script>

        </xpath>
    </template>
</odoo>