        <field name="code">SITAX_OS_SUM</field>
        <field name="sequence" eval="1"/>
        <field name="level" eval="0"/>
        <field name="formulas">balance = SITAX_OS_NEODB.balance + SITAX_OS_ODB.balance</field>
