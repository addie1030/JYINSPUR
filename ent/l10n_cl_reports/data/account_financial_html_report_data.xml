        <field name="domain" eval="[('tax_line_id.tag_ids', 'in', [ref('l10n_cl.tag_cl_09')])]"/>
        <field name="formulas">balance = - sum.balance</field>
        <field name="parent_id" ref="financial_report_line_cl_0202"/>
