        <record id="action_view_statements_monthend" model="ir.actions.server" >
            <field name="name">月末存档</field>
            <field name="model_id" ref="model_ps_statement_statements"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">
                action=env['ps.statement.statements'].monthend(env.context.get('active_ids'))
            </field>
            <field name="binding_model_id" ref="model_ps_statement_statements" />
        </record>

        <record id="action_view_statements_archive" model="ir.actions.server" >
            <field name="name">报表封存</field>
            <field name="model_id" ref="model_ps_statement_statements"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">
                action=env['ps.statement.statements'].archive(env.context.get('active_ids'))
            </field>
            <field name="binding_model_id" ref="model_ps_statement_statements" />
        </record>

        <record id="action_view_statements_unarchive" model="ir.actions.server" >
            <field name="name">报表启封</field>
            <field name="model_id" ref="model_ps_statement_statements"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">
                action=env['ps.statement.statements'].unarchive(env.context.get('active_ids'))
            </field>
            <field name="binding_model_id" ref="model_ps_statement_statements" />
        </record>

        <record id="view_statement_statements_newstatement" model="ir.ui.view">
            <field name="name">ps.statement.statements.newstatement.form</field>
            <field name="model">ps.statement.statements.newstatement</field>
            <field name="arch" type="xml">
                <form string="新建报表">
                    <group col="4">
                        <field name="report_code" placeholder="编号为4位字符"/>
                        <field name="report_name"/>
                        <field name="category"/>
                        <field name="title_rows"/>
                        <field name="head_rows"/>
                        <field name="body_rows"/>
                        <field name="tail_rows"/>
                        <field name="total_cols"/>
                    </group>
                    <footer>
                        <button string="新建" name="new_statement" type="object" class="btn-primary"/>
                        <button string="取消" class="btn-default" special="cancel"/>
                    </footer>
               </form>
            </field>
        </record>

