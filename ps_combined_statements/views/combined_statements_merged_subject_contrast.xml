<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<record id="merged_subject_contrast_tree" model="ir.ui.view">
		    <field name="name">merged.subject.contrast</field>
		    <field name="model">ps.combined.statements.merged.subject.contrast</field>
		    <field name="arch" type="xml">
		        <tree editable="bottom">
					<field name="debit_subject" options='{"no_open": True, "no_create": True}'/>
		            <field name="credit_subject" options='{"no_open": True, "no_create": True}'/>
					<field name="ir_model_id" options='{"no_open": True, "no_create": True}'/>
					<field name="field_id" options='{"no_open": True, "no_create": True}'/>
					<field name="field_id1" options='{"no_open": True, "no_create": True}'/>
					<field name="re_cash" options='{"no_open": True, "no_create": True}'/>
					<field name="re_cash1" options='{"no_open": True, "no_create": True}'/>
				</tree>
		    </field>
		</record>

		<record id="merged_subject_contrast_search" model="ir.ui.view">
			<field name="name">merged subject contrast search</field>
			<field name="model">ps.combined.statements.merged.subject.contrast</field>
			<field name="arch" type="xml">
				<search>
					<field name="debit_subject"/>
					<field name="credit_subject"/>
					<field name="ir_model_id"/>
					<group expand="0" string="Table name">
						<filter name="group_by_ir_model_id" string="Table name" domain="[]"
								context="{'group_by':'ir_model_id'}" />
					</group>

				</search>
			</field>
		</record>

		<record id="merged_subject_contrast_act_window" model="ir.actions.act_window">
		    <field name="name">Consolidated subject control table</field>
		    <field name="type">ir.actions.act_window</field>
		    <field name="res_model">ps.combined.statements.merged.subject.contrast</field>
		    <field name="view_mode">tree</field>
		</record>


		<menuitem id="merged_subject_contrast_menu"
				  name="Contrast table of offset relationship"
				  parent="combined_statements_merger_basic_record"
				  action="merged_subject_contrast_act_window"
				  sequence="1"/>

    </data>
</odoo>