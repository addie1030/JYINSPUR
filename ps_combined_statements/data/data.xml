<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

		<!--合并报表默认小数-->
		<record id="combined_statements_decimal" model="ir.config_parameter">
			<field name="key">combined.statements.decimal</field>
			<field name="value">2</field>
		</record>

		<record id="sequence_combined_statements_subject_seq" model="ir.sequence">
			<field name="name">Combined Statement Subject Code</field>
			<field name="code">statements.statement.subject.code</field>
			<field name="prefix" />
			<field name="padding">5</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<record id="sequence_combined_statements_project_seq" model="ir.sequence">
			<field name="name">Combined Statement Code</field>
			<field name="code">statements.statement.code</field>
			<field name="prefix">CSJ</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<record id="sequence_respective_statements_project_seq" model="ir.sequence">
			<field name="name">Respective Statements Project Code</field>
			<field name="code">ps.respective.statements.project</field>
			<field name="prefix">GB</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!-- 合并报表小数定义 -->
		<record forcecreate="True" id="combined_statements_decimal" model="decimal.precision">
			<field name="name">Combined Statements Decimal</field>
			<field name="digits">2</field>
		</record>

		<record id="sequence_combined_statements_seq" model="ir.sequence">
			<field name="name">Combined Statement Version</field>
			<field name="code">statements.version</field>
			<field name="prefix">CS</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<record id="sequence_combined_elimination_seq" model="ir.sequence">
			<field name="name">elimination</field>
			<field name="code">statements.elimination</field>
			<field name="prefix">CSE</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!--工作底稿定义编号-->
		<record id="sequence_merge_manuscript_init_seq" model="ir.sequence">
			<field name="name">Merge manuscript init</field>
			<field name="code">ps.merge.manuscript.init</field>
			<field name="prefix">DGDY</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!--工作底稿编号-->
		<record id="sequence_working_paper_seq" model="ir.sequence">
			<field name="name">Working Paper Sequence</field>
			<field name="code">working.paper.seq</field>
			<field name="prefix">GZDG</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!--合并报表编号-->
		<record id="sequence_merge_statements_seq" model="ir.sequence">
			<field name="name">Merge Statements Sequence</field>
			<field name="code">merge.statements.seq</field>
			<field name="prefix">HBBB</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!--调整表生成-->
		<record id="adjust_working_paper_seq" model="ir.sequence">
			<field name="name">Adjust Working Paper</field>
			<field name="code">adjust.working.paper.seq</field>
			<field name="prefix">TZGZDG</field>
			<field name="padding">4</field>
			<field name="number_next">1</field>
			<field name="number_increment">1</field>
		</record>

		<!--变更类型-->
		<record id="budget_square" model="ps.change.type">
			<field name="name">暂停折旧</field>
			<field name="code">ZTZJ</field>
		</record>
		<!--变更类型-->
		<record id="budget_square" model="ps.change.type">
			<field name="name">恢复折旧</field>
			<field name="code">HFZJ</field>
		</record>
		<!--变更类型-->
		<record id="budget_square" model="ps.change.type">
			<field name="name">折旧月数变更</field>
			<field name="code">ZJYSBG</field>
		</record>

		<!--样表列类型-->
		<record id="journaling_template_column_mcl" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">名称列</field>
			<field name="is_display" eval="False"/>
			<field name="description">用于显示项目名称</field>
		</record>

		<record id="journaling_template_column_hcl" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">行次列</field>
			<field name="is_display" eval="False" />
			<field name="description">用于显示项目所对应的行号</field>
		</record>

		<record id="journaling_template_column_ncs" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">年初数</field>
			<field name="is_display" eval="True" />
			<field name="description">年初数</field>
		</record>

		<record id="journaling_template_column_qms" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">期末数</field>
			<field name="is_display" eval="True" />
			<field name="description">期末数</field>
		</record>

		<record id="journaling_template_column_bqfss" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">本期发生数</field>
			<field name="is_display" eval="True" />
			<field name="description">本期发生数</field>
		</record>

		<record id="journaling_template_column_bnljs" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">本年累计数</field>
			<field name="is_display" eval="True" />
			<field name="description">本年累计数</field>
		</record>

		<record id="journaling_template_column_bnzjbdje" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">本年增减变动金额</field>
			<field name="is_display" eval="True" />
			<field name="description">本年增减变动金额</field>
		</record>

		<record id="journaling_template_column_sntqqms" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">上年同期期末数</field>
			<field name="is_display" eval="True" />
			<field name="description">上年同期期末数</field>
		</record>

		<record id="journaling_template_column_sntqfss" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">上年同期发生数</field>
			<field name="is_display" eval="True" />
			<field name="description">上年同期发生数</field>
		</record>

		<record id="journaling_template_column_sqfss" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">上期发生数</field>
			<field name="is_display" eval="True" />
			<field name="description">上期发生数</field>
		</record>

		<record id="journaling_template_column_bqzjs" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">本期增加数</field>
			<field name="is_display" eval="True" />
			<field name="description">本期增加数</field>
		</record>

		<record id="journaling_template_column_bqjss" model="ps.combined.statements.journaling.template.columntype">
			<field name="name">本期减少数</field>
			<field name="is_display" eval="True" />
			<field name="description">本期减少数</field>
		</record>
		<!--抵消类型-->
		<record id="merge_elimination_entry_type1" model="ps.merge.elimination.entry.type">
			<field name="name">债权债务抵消</field>
			<field name="model">ps.combined.statements.inside.credit.debt</field>
		</record>

		<record id="merge_elimination_entry_type2" model="ps.merge.elimination.entry.type">
			<field name="name">常见交易抵消</field>
			<field name="model">ps.combined.statements.inside.familiar.deal</field>
		</record>

		<record id="merge_elimination_entry_type3" model="ps.merge.elimination.entry.type">
			<field name="name">存货抵消</field>
			<field name="model">combined.statements.inside.inventory.deal</field>
		</record>

		<record id="merge_elimination_entry_type4" model="ps.merge.elimination.entry.type">
			<field name="name">长期资产抵消</field>
			<field name="model">ps.combined.statements.inside.asset.deal</field>
		</record>

		<record id="merge_elimination_entry_type5" model="ps.merge.elimination.entry.type">
			<field name="name">权益类抵消分录生成</field>
			<field name="model">ps.subsidiary.profit.statement</field>
		</record>

    </data>
</odoo>