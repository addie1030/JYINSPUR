<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

		<record id="dialog_info_wizard" model="ir.ui.view">
			<field name="name">ps.dialog.info.wizard</field>
			<field name="model">ps.dialog.info.wizard</field>
			<field name="arch" type="xml">
				<form>
					<field name="info" nolabel="1"/>
					<footer>
						<button string="Confirm view" name="check_models" type="object" default_focus="1" class="btn-primary" />
						<button string="cancel" class="btn-default" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

    </data>
</odoo>