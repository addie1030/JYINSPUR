<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data>

        <record id="ps_account.tri_ledger_view" model="ir.actions.client">
            <field name="name">Account ledger</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.tri.ledger.report'}" />
        </record>

        <record id="ps_account.balance_view" model="ir.actions.client">
            <field name="name">Account balance account</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.balance.report'}" />
        </record>

        <record id="ps_account.general_ledger_view" model="ir.actions.client">
            <field name="name">General ledger</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.general.ledger.report'}" />
        </record>

        <record id="ps_account.detail_view" model="ir.actions.client">
            <field name="name">Partner Detail Ledger</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.auxiliary.detail.report'}" />
        </record>

        <record id="ps_account.fzyeb_view" model="ir.actions.client">
            <field name="name">Partner balance sheet</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.fzyeb.report'}" />
        </record>

        <record id="ps_account.multi_column_account_view" model="ir.actions.client">
            <field name="name">Multi column account</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.multi.column.account.report'}" />
        </record>
        <record id="ps_account.summary_view" model="ir.actions.client">
            <field name="name">Account summary account</field>
            <field name="tag">account_china_report</field>
            <field name="context" eval="{'model': 'account.china.summary.report'}" />
        </record>

        <menuitem id="menu_kmmxz_view"
          name="Account ledger"
          parent="ps_account.menu_ps_finance_query"
          sequence="14"
          action="ps_account.tri_ledger_view"
          groups="account.group_account_manager"
        />

        <menuitem id="menu_kmyeb_view"
          name="Account balance account"
          parent="ps_account.menu_ps_finance_query"
          sequence="15"
          action="ps_account.balance_view" groups="account.group_account_manager"/>

        <menuitem id="menu_general_ledger_view"
          name="General ledger"
          parent="ps_account.menu_ps_finance_query"
          sequence="16"
          action="ps_account.general_ledger_view" groups="account.group_account_manager"/>

        <menuitem id="menu_multi_column_account_view"
          name="Multi column account"
          parent="ps_account.menu_ps_finance_query"
          sequence="17"
          action="ps_account.multi_column_account_view" groups="account.group_account_manager"/>

        <menuitem id="menu_detail_view"
          name="Partner Detail Ledger"
          parent="ps_account.menu_ps_finance_query"
          sequence="18"
          action="ps_account.detail_view" groups="account.group_account_manager"/>

        <menuitem id="menu_fzyeb_view"
          name="Partner balance sheet"
          parent="ps_account.menu_ps_finance_query"
          sequence="19"
          action="ps_account.fzyeb_view" groups="account.group_account_manager"/>

        <menuitem id="menu_kmhzb_view"
          name="Account summary account"
          parent="ps_account.menu_ps_finance_query"
          sequence="0"
          action="ps_account.summary_view" groups="account.group_account_manager"/>

    </data>
