<?xml version="1.1" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_move_form_new" model="ir.ui.view">
            <field name="name">account.move.form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="replace" >
                <form string="Account Entry" class="o_form_view_inspur">
                    <header>
                        <field name="state" widget="statusbar" statusbar_visible="draft,checked,posted"/>
                    </header>
                    <sheet>

                    <h1 style="width:100%;text-align:center;position:relative;"><!--underline;-->
                        <field name="ps_voucher_name" readonly="True"/>
                        <div id="ps_switch" style="position:absolute;top: 5px;right: 0px;width:150px;text-align:center;">
                            <p style="width: 150px;font-size: 10px; margin:0px;">The shortcut key for switch to debit/credit is spacebar</p>
                        </div>
                    </h1>
                    <group>
                        <group col="10">
                            <field name="journal_id" string="Account book:" options="{'no_open': True, 'no_create': True}"
                                   context="{'ps_voucher_word':ps_voucher_word,
                                             'date':date,
                                             'journal_id':journal_id
                                             }"
                            attrs="{'readonly': ['|',('state','=','checked'),('state','=','posted')]}"/>
                            <field name="date" string="Billing date:" context="{'date':date}"
                            attrs="{'readonly': ['|',('state','=','checked'),('state','=','posted')]}"/>
                            <field name="ps_voucher_word" string="voucher word：" readonly="1" context="{'ps_voucher_word':ps_voucher_word}"/>
                            <field name="name" string="number:"  readonly="1"/>
                            <field name="ps_attachcount" string="Number of attachments:" required="1"
                            attrs="{'readonly': ['|',('state','=','checked'),('state','=','posted')]}"/>
                            <field name="company_id" invisible="1" required="0" groups="base.group_multi_company"/>
                            <field name="currency_id" invisible="1"/>
                        </group>
                    </group>
                            <field name="line_ids" widget="one2many_list"
                              context="{'line_ids': line_ids, 'journal_id': journal_id }">
                                <tree editable="bottom" string="Journal Items"  class="inspur_account">
                                    <field name="name" string="Summary" required="1"
                                    style="white-space: pre-wrap;"/>
                                    <field name="account_id" domain="[('company_id', '=', parent.company_id), ('deprecated', '=', False), ('ps_is_leaf', '=', True)]"
                                            options="{'no_create_edit': True}"
                                    />
                                    <field name="partner_id" invisible="1"
                                        domain="['|', ('parent_id', '=', False), ('is_company', '=', True)]"/>
                                    <field name="ps_sub_id" context="{'auxiliary_account_id':account_id,
                                                                         'ps_is_sub':ps_is_sub,
                                                                         }"
                                           domain="[('state', '=', 'new')]"
                                           attrs="{'readonly': [('ps_is_sub', '=', False)],'required': [('ps_is_sub', '=', True)]}"
                                    />
                                    <field name="analytic_account_id" invisible="1" string="Auxiliary accounting" groups="analytic.group_analytic_accounting"/>
                                    <field name="analytic_tag_ids" invisible="1" groups="analytic.group_analytic_accounting"/>
                                    <field name="company_currency_id" invisible="1"/>
                                    <field name="company_id" invisible="1" string="partner" />
                                    <field name="product_id" invisible="1" string="product"/>
                                    <field name="product_uom_id" invisible="1" string="unit"/>
                                    <field name="quantity" invisible="1" string="quantity"/>
                                    <field name="currency_id"  invisible="1" string="currency" options="{'no_create': True}" groups="base.group_multi_currency"/>
                                    <field name="amount_currency" invisible="1" string="amount" groups="base.group_multi_currency"/>
                                    <field name="debit" class="rel rel_debit" string="debit" sum="Total Debit" />
                                    <field name="credit" class="rel rel_credit" string="credit" sum="Total Credit" />
                                    <field name="date_maturity" required="0" invisible="1"/>
                                    <field name="ps_is_sub" invisible="1" context="{'ps_is_sub':ps_is_sub}"/>
                                </tree>
                            </field>
                        <group>
                            <group col="6">
                                <field name="create_uid" readonly="1" string="created user："/>
                                <field name="ps_confirmed_user" readonly="1" string="confirmed user："/>
                                <field name="ps_posted_user" readonly="1" string="posted user："/>
                            </group>
                        </group>
                    </sheet>
                </form>
                </xpath>
            </field>
        </record>

        <record id="view_move_line_form_new" model="ir.ui.view">
            <field name="name">account.move.line.form</field>
            <field name="model">account.move.line</field>
            <field eval="2" name="priority"/>
            <field name="inherit_id" ref="account.view_move_line_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="replace" >
                    <form string="Journal Item" create="false" edit="false" delete="false">
                    <sheet>
                        <field name="company_id" invisible="1"/>
                        <field name="parent_state" invisible="1"/>
                        <field name="ps_is_sub" invisible="1"/>
                        <group>
                            <field name="name"/>
                            <field name="partner_id"
                                domain="['|', ('parent_id', '=', False), ('is_company', '=', True)]"/>
                        </group>
                        <notebook colspan="4">
                            <page string="Information">
                                <group>
                                    <group string="Amount">
                                        <field name="account_id" domain="[('company_id', '=', company_id)]" attrs="{'readonly':[('parent_state','!=','draft')]}"/>
                                        <field name="debit" attrs="{'readonly':[('parent_state','!=','draft')]}"/>
                                        <field name="credit" attrs="{'readonly':[('parent_state','!=','draft')]}"/>
                                        <field name="quantity"/>
                                    </group>
                                    <group string="Accounting Documents">
                                        <field name="ref" string="Document No" attrs="{'readonly':[('parent_state','!=','draft')]}"/>
                                        <field name="statement_id" readonly="True" attrs="{'invisible': [('statement_id','=',False)]}"/>
                                        <field name="invoice_id" attrs="{'invisible': [('invoice_id','=',False)]}"/>
                                    </group>
                                    <group string="Dates">
                                        <field name="date_maturity"/>
                                    </group>

                                    <group string="Taxes" attrs="{'invisible': [('tax_line_id','=',False), ('tax_ids','=',[])]}">
                                        <field name="tax_line_id" readonly="1" attrs="{'invisible': [('tax_line_id','=',False)]}"/>
                                        <field name="tax_ids" widget="many2many_tags" readonly="1" attrs="{'invisible': [('tax_ids','=',[])]}"/>
                                    </group>
                                    <group string="Matching" attrs="{'invisible':[('full_reconcile_id','=',False)]}">
                                        <label for="full_reconcile_id"/>
                                        <div>
                                            <field name="full_reconcile_id"/>
                                            <field name="matched_debit_ids" invisible="1"/>
                                            <field name="matched_credit_ids" invisible="1"/>
                                            <button name="open_reconcile_view"
                                                class="oe_link"
                                                type="object"
                                                string="-> View partially reconciled entries"
                                                attrs="{'invisible': ['|', ('full_reconcile_id', '!=', False), '&amp;', ('matched_debit_ids', '=', []),('matched_credit_ids', '=', [])]}">
                                            </button>
                                        </div>
                                    </group>
                                    <group string="Currency" groups="base.group_multi_currency">
                                        <field name="currency_id" invisible="1"/>
                                        <field name="amount_currency"/>
                                    </group>
                                    <group string="States">
                                        <field name="blocked"/>
                                    </group>
                                    <group groups="analytic.group_analytic_accounting" string="Analytic">
                                        <field name="analytic_account_id"/>
                                        <field name="analytic_tag_ids" widget="many2many_tags"/>
                                    </group>
                                </group>
                                <field name="narration" colspan="4" nolabel="1" placeholder="Add an internal note..."/>
                            </page>
                            <page string="Analytic Lines" groups="analytic.group_analytic_accounting">
                                <field name="date" invisible="1"/>
                                <field name="analytic_line_ids" context="{'default_general_account_id':account_id, 'default_name': name, 'default_date':date, 'amount': (debit or 0.0)-(credit or 0.0)}"/>
                            </page>
                        </notebook>
                    </sheet>
                    </form>
                </xpath>
            </field>
        </record>

        <record id="view_account_list_new" model="ir.ui.view">
            <field name="name">account.account.list</field>
            <field name="model">account.account</field>
            <field name="inherit_id" ref="account.view_account_list"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="before">
                    <field name="account_attribute_id" />
                </xpath>
            </field>
        </record>


        <record id="view_account_form_new" model="ir.ui.view">
            <field name="name">account.account.form</field>
            <field name="model">account.account</field>
            <field name="inherit_id" ref="account.view_account_form"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="replace" >
                <form string="Account" class="all_print_remove">
                    <header>
                          <button name="create_new_account"  string="Generate child" type="object"
                                  class="oe_highlight" attrs="{'invisible': [('code', '=', False)]}"
                                  groups="account.group_account_invoice"
                                  context="{'par_account':'child_level'}"/>
                          <button name="create_new_account"  string="Generate peer" type="object"
                                  class="oe_highlight" attrs="{'invisible': [('code', '=', False)]}"
                                  groups="account.group_account_invoice"
                                  context="{'par_account':'same_level'}"/>
                    </header>
                   <sheet>
                       <group>
                           <group>
                             <field name="parent_id" options="{'no_create': True}"/>
                               <field name="ps_is_leaf" invisible="1"/>
                             <field name="code" string="Account number" />
                             <field name="name" string="Account name" />
                             <field name="ps_shortcode"/>
                             <field name="account_attribute_id" required="True" options="{'no_create_edit': True,'no_open': True}"/>
                             <field name="group_id" invisible="1"/>
                             <field name="company_id" options="{'no_create': True}" groups="base.group_multi_company"/>
                             <field name="internal_type" invisible="1" readonly="1"/>
                             <field name="currency_id" options="{'no_create': True}"
                                      groups="base.group_multi_currency"/>
                           </group>
                           <group>
                               <field name="user_type_id" widget="selection"/>
                               <field name="tax_ids" widget="many2many_tags"/>
                               <field name="ps_balance_direction"/>
                               <label for="reconcile"/>
                               <div>
                                <field name="reconcile"/>
                                  <button name="action_open_reconcile" class="oe_link" type="object"
                                          string=" -> Reconcile" attrs="{'invisible': [('reconcile', '=', False)]}"/>
                               </div>
                               <field name="deprecated" string="Seal"/>
                           </group>
                           <group string="Auxiliary accounting">
                               
                               <field name="ps_consider_partner"/>
                               <field name="ps_consider_product"/>
                               <field name="ps_is_cash_flow"/>
                               <field name="ps_is_account_analytic" groups="analytic.group_analytic_accounting"/>
                               <field name="ps_analytic_tag_id"
                                      attrs="{'invisible': [('ps_is_account_analytic', '=', False)],
                                              'required': [('ps_is_account_analytic', '=', True)]}"
                               />
                           </group>
                       </group>
                    </sheet>
                </form>
                </xpath>
            </field>
        </record>

        <record id="view_account_search_new" model="ir.ui.view">
            <field name="name">account.account.search</field>
            <field name="model">account.account</field>
            <field name="inherit_id" ref="account.view_account_search"/>
            <field name="arch" type="xml">
                <xpath expr="//search" position="replace">
                    <search string="Accounts">
                        <field name="name" filter_domain="['|', ('name','ilike',self), ('code','=like',str(self)+'%')]" string="Account"/>
                        <filter name="receivale_account" string="Receivable Accounts" domain="[('internal_type','=','receivable')]"/>
                        <filter name="payable_account" string="Payable Accounts" domain="[('internal_type','=','payable')]"/>
                        <field name="user_type_id"/>
                        <field name="account_attribute_id"/>
                        <group expand="0" string="Group By">
                            <filter name="account_type" string="Account type" domain="" context="{'group_by':'user_type_id'}"/>
                            <filter name="account_group" string="Account attribute" domain="" context="{'group_by':'account_attribute_id'}" />
                        </group>
                    </search>
                </xpath>
            </field>
        </record>

        <record id="account.action_account_form" model="ir.actions.act_window">

            <field name="search_view_id" ref="view_account_search_new"/>

        </record>

        <!--辅助核算-->
        <record id="setup_ps_account_auxiliary_tree" model="ir.ui.view">
            <field name="name">ps.account.move.line.sub.tree</field>
            <field name="model">ps.account.move.line.sub</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Account Auxiliary Items" ><!--decoration-info="is_unaffected_earnings_line == True"-->
                    <field name="ps_consider_partner" options="{'no_create': True}"/>
                    <field name="ps_consider_product" options="{'no_create': True}"/>
                    <field name="ps_consider_currency" options="{'no_create': True}"/>
                    <field name="product_uom_id" options="{'no_create': True}"/>
                    <field name="ps_consider_quantity"/>
                    <field name="unit_price"/>
                    <field name="amount"/>
                    <field name="debit" sum="Total Debit"/>
                    <field name="credit" sum="Total Credit"/>
                </tree>
            </field>
        </record>

        <record id="ps_account_auxiliary_form_view_id" model="ir.ui.view">
            <field name="name">ps.account.move.line.sub.form</field>
            <field name="model">ps.account.move.line.sub</field>
            <field name="arch" type="xml">
                <form string="Auxiliary accounting">
                    <sheet>
                        <group>
                            <group>
                                <field name="ps_consider_partner" invisible="0" required="1" options="{'no_create_edit': True}"/>
                                <field name="ps_consider_product" invisible="0" required="1" options="{'no_create_edit': True}"/>
                                <field name="product_uom_id" invisible="0" required="1" options="{'no_create_edit': True}"/>
                                <field name="ps_consider_currency" invisible="0" required="1" options="{'no_create_edit': True}"/>
                                <field name="ps_currency_rate" invisible="0" />
                                <field name="ps_consider_quantity" invisible="0" required="1"/>
                                <field name="unit_price" invisible="0" required="1"/>
                                <field name="amount_input" invisible="0"/>
                                <field name="amount"  invisible="0"/>
                                <field name="cash_flow_item_id" required="1" options="{'no_create_edit': True}"/>
                                <field name="account_id" invisible="1"/>
                            </group>
                        </group>
                        <field name="analytic_line_ids">
                            <tree editable="bottom">
                                <field name="account_id" options="{'no_create_edit': True}"/>
                                <field name="tag_id" readonly="1"/>
                                <field name="amount" sum="Total"/>
                                <field name="name" invisible="1"/>
                                <field name="ps_account_id" invisible="1"/>
                            </tree>
                        </field>
                    </sheet>
                </form>
            </field>
        </record>


        <record id="ps_account_auxiliary_action_id" model="ir.actions.act_window">
            <field name="name">Auxiliary accounting</field>
            <field name="res_model">ps.account.move.line.sub</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Please create a secondary accounting record.
              </p>
            </field>
        </record>


        <record id="view_ps_account_move_check_search" model="ir.ui.view">
            <field name="name">ps.account.move.check.search</field>
            <field name="model">ps.account.move.check</field>
            <field name="arch" type="xml">
                    <search string="Accounts">
                        <field name="ref" filter_domain="[('ref','=',self)]" string="Document No"/>
                        <field name="name" filter_domain="[('name','ilike',self)]" string="Document summary"/>
                        <filter string="Unreviewed" domain="[('state','=','draft')]" name="ps_draft" context="{'default_state': 'draft'}"/>
                        <filter string="Reviewed" domain="[('state','=','checked')]" name="ps_reviewd" />
                    </search>
            </field>
        </record>


        <record id="ps_account_move_check_view_tree_id" model="ir.ui.view">
        <field name="name">ps.account.move.check.view</field>
        <field name="model">ps.account.move.check</field>
        <field name="arch" type="xml">
                <tree class="o_form_view_inspur inspur_check" default_order='ref,date' decoration-danger="ps_check_sub_fail==True">
                    <field name="ref" />
                    <field name="journal_id" string="Document Type"/>
                    <field name="date" string="Document date"/>
                    <field name="name" />
                    <field name="account_id" />
                    <field name="ps_sub_id" />
                    <field name="debit"/>
                    <field name="credit"/>
                    <field name="ps_confirmed_user"/>
                    <field name="ps_confirmed_datetime"/>
                    <field name="state" />
                    <field name="ps_check_sub_fail" invisible="1"/>
                </tree>
        </field>
        </record>

        <record id='action_ps_account_move_check' model='ir.actions.act_window' >
            <field name='name'>Document review</field>
            <field name='res_model'>ps.account.move.check</field>
            <field name='view_mode'>tree</field>
            <field name="context">{'search_default_ps_draft':1, 'remove_name': 'ps_list_actions_delete'}</field>
            <field name='view_id' ref='ps_account_move_check_view_tree_id' />
            <field name="search_view_id" ref="view_ps_account_move_check_search"/>
        </record>
        <menuitem id="ps_account_move_check_menu"
              name="Document review"
              parent="menu_ps_move_patch_handle"
              action="action_ps_account_move_check"
              sequence="0"
              groups="account.group_account_manager"/>

        <!--凭证记账-->
        <record id="view_ps_account_move_post_search" model="ir.ui.view">
            <field name="name">ps.account.move.post.search</field>
            <field name="model">ps.account.move.post</field>
            <field name="arch" type="xml">
                    <search string="Accounts">
                        <field name="ref" filter_domain="[('ref','=',self)]" string="Document No"/>
                        <field name="name" filter_domain="[('name','ilike',self)]" string="Document summary"/>
                        <filter string="Unrecorded" domain="[('state','=','checked')]" name="ps_checked" context="{'default_state': 'checked'}"/>
                        <filter string="Recorded" domain="[('state','=','posted')]" name="ps_recorded" />
                    </search>
            </field>
        </record>

        <record id="ps_account_move_post_view_tree_id" model="ir.ui.view">
        <field name="name">ps.account.move.post.view</field>
        <field name="model">ps.account.move.post</field>
        <field name="arch" type="xml">
                <tree class="o_form_view_inspur inspur_check" default_order='ref,date'>
                    <field name="ref" />
                    <field name="journal_id" string="Document type"/>
                    <field name="date" string="Document date"/>
                    <field name="name" />
                    <field name="account_id" />
                    <field name="ps_sub_id" />
                    <field name="debit"/>
                    <field name="credit"/>
                    <field name="ps_posted_user"/>
                    <field name="ps_posted_datetime"/>
                    <field name="state" />
                </tree>
        </field>
        </record>

        <record id='action_ps_account_move_post' model='ir.actions.act_window' >
            <field name='name'>Voucher accounting</field>
            <field name='res_model'>ps.account.move.post</field>
            <field name='view_mode'>tree</field>
            <field name="context">{'search_default_ps_checked':1, 'remove_name': 'ps_list_actions_delete'}</field>
            <field name='view_id' ref='ps_account_move_post_view_tree_id' />
            <field name="search_view_id" ref="view_ps_account_move_post_search"/>
        </record>
        <menuitem id="ps_account_move_post_menu"
              name="Certificate accounting"
              parent="menu_ps_move_patch_handle"
              action="action_ps_account_move_post"
              sequence="0"
              groups="account.group_account_manager"/>

        <record id="init_accounts_tree_new" model="ir.ui.view">
            <field name="name">account.setup.opening.move.line.tree</field>
            <field name="model">account.account</field>
            <field name="inherit_id" ref="account.init_accounts_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//tree" position="replace" >
                    <tree editable="top" create="0" delete="0"
                          decoration-info="ps_auxiliary_state=='1'"
                          decoration-bf="ps_is_leaf==False"
                          decoration-it="ps_is_leaf==False"
                          decoration-danger="ps_is_leaf==False"
                    >
                        <field name="account_attribute_id" readonly="1"/>
                        <field name="code" readonly="1"/>
                        <field name="name" readonly="1"/>
                        <field name="ps_balance_direction" readonly="1"/>
                        <field name="ps_auxiliary_state" readonly="1" invisible="1"
                               attrs="{'invisible': [('ps_auxiliary_state','=','0')]}"
                               string="Whether it is auxiliary accounting"
                        />
                        <field name="ps_sub_id"
                               attrs="{'readonly': [('ps_auxiliary_state','=','0')],'required':[('ps_auxiliary_state','=','1')]}"
                               domain="[('state', '=', 'new')]"
                               invisible="1"
                        />
                        <button string="Auxiliary accounting" name="open_ps_auxiliary_list"  type="object"
                                attrs="{'invisible': [('ps_auxiliary_state','=','0')]}"
                                context="{'auxiliary_account_id':id}"
                                icon="fa-dedent fa-2x"
                        />
                        <field name="opening_debit"
                               attrs="{'invisible': [('ps_is_leaf','!=',True)],'readonly': [('ps_auxiliary_state','=','1')]}"/>
                        <field name="opening_credit"
                               attrs="{'invisible': [('ps_is_leaf','!=',True)],'readonly': [('ps_auxiliary_state','=','1')]}"/>
                        <field name="ps_is_leaf" invisible="1" string="Whether the final stage"/>
                        <field name="id" invisible="1" string="Account ID"/>
                    </tree>
                </xpath>
            </field>
        </record>

        <record id="init_accounts_form_new" model="ir.ui.view">
            <field name="name">account.setup.opening.move.line.form</field>
            <field name="model">account.account</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>

                    <h1 style="width:100%;text-align:center;text-decoration:"><!--underline;-->
                            Beginning of the subject
                    </h1>
                        <button name="open_win"  string="assets" type="object" class="oe_highlight" groups="account.group_account_invoice"/>
                        <button name="open_win"  string="Liabilities" type="object"  groups="account.group_account_invoice"/>
                        <button name="open_win"  string="cost" type="object"  groups="account.group_account_invoice"/>
                        <button name="open_win"  string="rights and interests" type="object"  groups="account.group_account_invoice"/>
                        <button name="open_win"  string="profit and loss" type="object"  groups="account.group_account_invoice"/>
                        <button name="open_win"  string="common" type="object"  groups="account.group_account_invoice"/>
                        <notebook>
                            <page>
                                <tree editable="top" create="1" delete="1" decoration-info="ps_auxiliary_state=='1'">
                                    <field name="code" readonly="1"/>
                                    <field name="name" readonly="1"/>
                                    <field name="account_attribute_id" readonly="1" invisible="1"/>
                                    <field name="ps_balance_direction" readonly="1"/>
                                    <field name="ps_auxiliary_state" readonly="1" invisible="1"
                                           attrs="{'invisible': [('ps_auxiliary_state','=','0')]}"
                                           string="Whether it is auxiliary accounting"
                                    />
                                    <field name="ps_sub_id"
                                           attrs="{'readonly': [('ps_auxiliary_state','=','0')],'required':[('ps_auxiliary_state','=','1')]}"
                                           context="{'auxiliary_account_id':id}"
                                           domain="[('state', '=', 'new')]"
                                    />
                                    <button string="Auxiliary accounting" attrs="{'invisible': [('ps_auxiliary_state','=','0')]}"/>
                                    <field name="opening_debit" attrs="{'readonly': [('ps_is_leaf','!=',True)]}"/>
                                    <field name="opening_credit" attrs="{'readonly': [('ps_is_leaf','!=',True)]}"/>
                                    <field name="ps_is_leaf" invisible="1"/>
                                    <field name="id" invisible="1"/>
                                </tree>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

        <record id='action_ps_account_opening' model='ir.actions.act_window' >
            <field name='name'>Account balance initial</field>
            <field name='res_model'>account.account</field>
            <field name='view_mode'>tree</field>
            <field name="view_type">form</field>
            <field name="type">ir.actions.act_window</field>
            <field name='view_id' ref='init_accounts_tree_new' />
            <field name="context">{'remove_name': 'all_print_remove'}</field>
        </record>

        <menuitem id="account_ps_account_account_opening_menu"
              name="Account balance initial"
              parent="account.account_account_menu"
              action="action_ps_account_opening"
              sequence="0"
              groups="account.group_account_manager"/>


        <!-- <delete id="account.action_validate_account_move" model="ir.actions.act_window"/> -->
		<!-- <delete id="account.action_move_line_select_tax_audit" model="ir.actions.act_window"/> -->

        <record id="account.action_move_journal_line" model="ir.actions.act_window">
            <field name="name">Journal Entries</field>
            <field name="res_model">account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,kanban,form</field>
            <field name="view_id" ref="account.view_move_tree"/>
            <field name="search_view_id" ref="account.view_account_move_filter"/>
            <field name="context">{'search_default_misc_filter':1, 'view_no_maturity': True,'manual_move':'1'}</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to create a journal entry.
              </p>
              <p>
                A journal entry consists of several journal items, each of
                which is either a debit or a credit transaction.
              </p>
              <p>
                Odoo automatically creates one journal entry per accounting
                document: invoice, refund, vendor payment, bank statements,
                etc. So, you should record journal entries manually only/mainly
                for miscellaneous operations.
              </p>
            </field>
        </record>

        <record id="ps_invoice_tree" model="ir.ui.view">
            <field name="name">ps.account.invoice.tree</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_tree"/>
            <field name="arch" type="xml">
                <field name="name" position="attributes">
                    <attribute name="invisible"></attribute>
                </field>
            </field>
        </record>

    </data>
</odoo>
