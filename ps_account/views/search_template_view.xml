            <!--<t t-if="options.get('all_entries') != None" groups="account.group_account_user">-->
                <!--<t t-if="options['all_entries']">With Draft Entries,</t>-->
                <!--<t t-if="not options['all_entries']">Posted Entries Only,</t>-->
            <!--</t>-->
