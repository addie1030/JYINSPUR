        <ul class="dropdown-menu o_filters_menu" role="menu">
            <t t-foreach="options['account_state']" t-as="a">
                <li t-att-title="a.get('name')" t-att-data-id="a.get('id')" data-filter="account_state" class="js_account_china_report_account_state_filter"><a><t t-esc="a.get('name')"/></a></li>
            </t>
        </ul>
    </template>

    <template id="search_template_auxiliary">
        <a type="button" class="dropdown-toggle" data-toggle="dropdown">
            <span class="fa fa-filter"/> Auxiliary:
            <span class="caret"/>
        </a>
        <ul class="dropdown-menu o_filters_menu" role="menu">
            <li title="All" class="js_account_china_report_auxiliary_filter" data-filter="all"><a>All</a></li>
            <li title="Partner" class="js_account_china_report_auxiliary_filter" data-filter="partner"><a>Partner</a></li>
            <li title="Product" class="js_account_china_report_auxiliary_filter" data-filter="product"><a>Product</a></li>
            <li title="Cash Flow" class="js_account_china_report_auxiliary_filter" data-filter="cashflow"><a>Cash Flow</a></li>
        </ul>
    </template>

    <template id="search_template_china">
        <div class="btn-group o_dropdown o_account_reports_filter_date" t-if="options.get('date') != None">
            <t t-call="ps_account.search_template_date">
            </t>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_date_cmp" t-if="options.get('comparison') != None">
            <t t-call="account_reports.search_template_comparison"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_bool" t-if="options.get('cash_basis') != None or options.get('all_entries') != None or options.get('unfold_all') != None">
            <t t-call="account_reports.search_template_extra_options"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_account_state" t-if="options.get('account_state') != None">
            <t t-call="ps_account.search_template_account_state"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_subject" t-if="options.get('subject') != None">
            <t t-call="ps_account.search_template_subject"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_unit" t-if="options.get('unit') != None">
            <t t-call="ps_account.search_template_unit"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_partner" t-if="options.get('partner') != None">
            <t t-call="ps_account.search_template_partner"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_styles" t-if="options.get('style') != None">
            <t t-call="ps_account.search_template_styles"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_multi_company" t-if="options.get('multi_company') != None">
            <t t-call="account_reports.search_template_multi_company"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_journals" t-if="options.get('journals') != None">
            <t t-call="account_reports.search_template_journals"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_account_type" t-if="options.get('account_type') != None">
            <t t-call="account_reports.search_template_account_type"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_analytic" t-if="options.get('analytic') != None">
            <t t-call="account_reports.search_template_analytic"/>
        </div>
        <div class="btn-group o_dropdown o_account_reports_filter_auxiliary" t-if="options.get('auxiliary') != None">
            <t t-call="ps_account.search_template_auxiliary"/>
        </div>
    </template>


    </data>
</odoo>