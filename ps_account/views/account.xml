<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_backend" name="account assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" type="text/css" href="/ps_account/static/src/css/account_view.css"/>
            <link rel="stylesheet" type="text/css" href="/ps_account/static/src/css/account_china.css"/>
            <link rel="stylesheet" type="text/scss" href="/ps_account/static/src/scss/inspur_account.scss"/>
            <link rel="stylesheet" type="text/css" href="/ps_account/static/src/css/month.css"/>
            <link rel="stylesheet" type="text/css" href="/ps_account/static/src/css/loss_account.css"/>
            <script type="text/javascript" src="/ps_account/static/src/js/tour.js"></script>
        </xpath>
        <xpath expr="//script[contains(@src, '/web/static/src/js/views/list/list_renderer.js')]" position="replace">
            <script type="text/javascript" src="/ps_account/static/src/js/inspur_account.js"></script>
        </xpath>
        <xpath expr="//script[contains(@src, '/web/static/src/js/views/list/list_editable_renderer.js')]" position="replace">
            <script type="text/javascript" src="/ps_account/static/src/js/inspur_act.js"></script>
        </xpath>
        <xpath expr="//script[contains(@src, '/web/static/src/js/views/form/form_renderer.js')]" position="replace">
            <script type="text/javascript" src="/ps_account/static/src/js/ps_account_form_renderer.js"></script>
        </xpath>
        <xpath expr="//script[contains(@src, '/account/static/src/js/bills_tree_upload.js')]" position="after">
            <script type="text/javascript" src="/ps_account/static/src/js/inspur_account_dashboard.js"></script>
        </xpath>

        <xpath expr="." position="inside">
            <script type="text/javascript" src="/ps_account/static/src/js/button.js"></script>
            <link rel="stylesheet" type="text/css" href="/ps_account/static/src/css/calculator.css"/>

        </xpath>
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/ps_account/static/src/js/month.js"></script>
            <script type="text/javascript" src="/ps_account/static/src/js/month_act.js"></script>
            <script type="text/javascript" src="/ps_account/static/src/js/loss_account.js"></script>
            <script type="text/javascript" src="/ps_account/static/src/js/account_china_reports.js"></script>
        </xpath>
    </template>

    <template id='template_tri_ledger_reports' inherit_id='ps_account.template_tri_ledger_report' primary="True">

    </template>
    <template id='template_balance_reports' inherit_id='ps_account.template_balance_report' primary="True">

    </template>

    <template id='template_general_ledger_reports' inherit_id='ps_account.template_general_ledger_report' primary="True">

    </template>

    <template id='template_detail_reports' inherit_id='ps_account.template_detail_report' primary="True">

    </template>

    <template id='template_fzyeb_reports' inherit_id='ps_account.template_fzyeb_report' primary="True">

    </template>

    <template id='template_multi_column_account_reports' inherit_id='ps_account.template_multi_column_account_report' primary="True">
    </template>

    <template id='template_summary_reports' inherit_id='ps_account.template_summary_report' primary="True">

    </template>
</odoo>
