<?xml version="1.0" encoding="UTF-8"?>
<template id="template" xml:space="preserve">
    <t t-name="ImportView.check_button">
        <!--<t t-if="widget.title==='凭证审核'">-->
            <button type="button" class="btn btn-sm btn-primary o_button_check" style="display:none;">
                Voucher Review
            </button>
            <button type="button" class="btn btn-sm btn-primary o_button_post" style="display:none;">
                Voucher Posting
            </button>
            <button type="button" class="btn btn-sm btn-primary o_button_uncheck" style="display:none;">
                Cancel Review
            </button>
            <button type="button" class="btn btn-sm btn-primary o_button_unpost" style="display:none;">
                Cancel Posting
            </button>
        <!--</t>-->
    </t>
    <t t-extend="ListView.buttons">
        <t t-jquery="button.o_list_button_add" t-operation="after">
            <t t-call="ImportView.check_button"/>
        </t>
    </t>

    <t t-extend="FormView.buttons">
        <t t-jquery="button.o_form_button_create" t-operation="after">
            <button type="button" class="btn btn-sm btn-primary o_button_calculator" style="display:none;">
                Calculator
            </button>
        </t>
        <t t-jquery="button.o_form_button_cancel" t-operation="after">
            <button type="button" class="btn btn-sm btn-primary o_button_calculator" style="display:none;">
                Calculator
            </button>
        </t>
    </t>


</template>
