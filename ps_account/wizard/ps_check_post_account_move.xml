<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="ps_check_account_move_view_auxiliary" model="ir.ui.view">
            <field name="name">Voucher review judgment auxiliary accounting</field>
            <field name="model">ps.check.post.account.move</field>
            <field name="arch" type="xml">
                <form string="Post Journal Entries">
                    <label string="The marked subject has auxiliary accounting, please fill in the auxiliary accounting in the corresponding certificate." for=""/>
                    <footer>
                        <button string="confirm" class="btn-primary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_ps_check_account_move_auxiliary" model="ir.actions.act_window">
            <field name="name">Voucher review judgment auxiliary accounting</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.check.post.account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="ps_check_account_move_view_auxiliary"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <!--<field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>-->
            <field name="binding_model_id" ref="ps_account.model_ps_account_move_check" />
        </record>
        <!--凭证审核-->
        <record id="ps_check_account_move_view" model="ir.ui.view">
            <field name="name">Certificate review</field>
            <field name="model">ps.check.post.account.move</field>
            <field name="arch" type="xml">
                <form string="Post Journal Entries">
                    <label string="Are you sure you want to check the selected voucher?" for=""/>
                    <footer>
                        <button string="Validate" name="check_account_move" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_ps_check_account_move" model="ir.actions.act_window">
            <field name="name">Certificate review</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.check.post.account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="ps_check_account_move_view"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>
            <field name="binding_model_id" ref="ps_account.model_ps_account_move_check" />
        </record>
        <!--ENd凭证审核-->

        <!--凭证记账-->
        <record id="ps_post_account_move_view" model="ir.ui.view">
            <field name="name">Certificate accounting</field>
            <field name="model">ps.post.account.move</field>
            <field name="arch" type="xml">
                <form string="Post Journal Entries">
                    <label string="Are you sure you want to book the selected vouchers?" for=""/>
                    <footer>
                        <button string="Posting" name="post_account_move" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_ps_post_account_move" model="ir.actions.act_window">
            <field name="name">Certificate accounting</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.post.account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="ps_post_account_move_view"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>
            <field name="binding_model_id" ref="ps_account.model_ps_account_move_post" />
        </record>
        <!--End凭证记账-->

        <!--取消审核-->
        <record id="ps_uncheck_account_move_view" model="ir.ui.view">
            <field name="name">Cancellation of audit</field>
            <field name="model">ps.uncheck.account.move</field>
            <field name="arch" type="xml">
                <form string="Post Journal Entries">
                    <label string="Are you sure you want to cancel the voucher selected?" for=""/>
                    <footer>
                        <button string="Confirm" name="uncheck_account_move" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_ps_uncheck_account_move" model="ir.actions.act_window">
            <field name="name">Cancellation of audit</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.uncheck.account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="ps_uncheck_account_move_view"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>
            <field name="binding_model_id" ref="ps_account.model_ps_account_move_check" />
        </record>
        <!--ENd取消审核-->

        <!--取消记账-->
        <record id="ps_unpost_account_move_view" model="ir.ui.view">
            <field name="name">Cancel the account</field>
            <field name="model">ps.unpost.account.move</field>
            <field name="arch" type="xml">
                <form string="Post Journal Entries">
                    <label string="Are you sure you want to cancel the selected credentials?" for=""/>
                    <footer>
                        <button string="Confirm" name="unpost_account_move" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_ps_unpost_account_move" model="ir.actions.act_window">
            <field name="name">Cancel the account</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ps.unpost.account.move</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="ps_unpost_account_move_view"/>
            <field name="context">{}</field>
            <field name="target">new</field>
            <field name="help">This wizard will validate all journal entries selected. Once journal entries are validated, you can not update them anymore.</field>
            <field name="binding_model_id" ref="ps_account.model_ps_account_move_post" />
        </record>
        <!--End取消记账-->

        <record id="setup_ps_account_opening_auxiliary_wizard_form" model="ir.ui.view">
            <field name="name">ps.account.opening.auxiliary.wizard.form</field>
            <field name="model">ps.sub.opening</field>
            <field name="arch" type="xml">
                <form>
                    <!--调到指定的form视图-->
                    <!--<field name="product_id" context="{'form_view_ref': 'odoo9_model.odoo9_product_hxy_form'}"> -->
                    <!--<field name="auxiliary_ids" context="{'tree_view_ref': 'ps_account.setup_ps_account_auxiliary_tree'}"/>-->
                    <field name="subopening_ids" widget="one2many_list">
                        <tree editable="bottom" string="Account Auxiliary Items" >
                            <field name="ps_consider_partner" options="{'no_create': True}" invisible="1"/>
                            <field name="ps_consider_product" options="{'no_create': True}" invisible="1"/>
                            <field name="ps_consider_currency" options="{'no_create': True}" invisible="1"/>
                            <field name="ps_currency_rate" invisible="1" />
                            <field name="product_uom_id" options="{'no_create': True}" invisible="1"/>
                            <field name="ps_consider_quantity" invisible="1"/>
                            <field name="unit_price" invisible="1"/>
                            <field name="cash_flow_item_id" invisible="1" options="{'no_create_edit': True}"/>
                            <field name="balance_direction" required="1"/>
                            <field name="amount_input" invisible="1"/>
                            <field name="amount" invisible="1"/>
                            <field name="account_id" invisible="1"/>
                        </tree>
                    </field>
                    <footer>
                        <button string="Confirm" type="object" name="ps_set_account_move_line" class="oe_highlight"/>
                        <button string="Cancel" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
