        <record forcecreate="True" id="0001_YEAR" model="ps.statement.system.variables">
            <field name="name">@YEAR@</field>
			<field name="description">Accounting year，2018</field>
            <field name="note"></field>
        </record>
		<record forcecreate="True" id="0002_MONTH" model="ps.statement.system.variables">
            <field name="name">@MONTH@</field>
			<field name="description">Accounting month，01</field>
            <field name="note"></field>
        </record>
		<record forcecreate="True" id="0003_DAY" model="ps.statement.system.variables">
            <field name="name">@DAY@</field>
