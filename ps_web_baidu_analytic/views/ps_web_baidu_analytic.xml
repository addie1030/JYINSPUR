<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend" name="web_analytics assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/ps_web_baidu_analytic/static/src/js/web_analytics.js"></script>
            </xpath>
            <xpath expr="//script[contains(@src, '/web/static/src/js/views/form/form_controller.js')]" position="replace">
                <script type="text/javascript" src="/ps_web_baidu_analytic/static/src/js/form_ex.js"></script>
            </xpath>
        </template>
    </data>
</odoo>